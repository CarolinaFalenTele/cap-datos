{
  "welcomeFile": "project1",
  "authenticationMethod": "route",
"sessionTimeout": 120,

  
  "routes": [
    {
      "source": "^/(.*)$",
      "target": "$1",
      "destination": "srv-api",
      "csrfProtection": true,
      "authenticationType": "xsuaa"
    },

    {
      "source": "^/user-api(.*)",
      "target": "$1",
      "service": "sap-approuter-userapi"
  }
  ]
}