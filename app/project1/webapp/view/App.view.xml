                                                                            id="button0"
                                                                            press="onActivityPress"
                                                                        >
                                                                            <customData
                                                                            >
                                                                                <core:CustomData
                                                                                    xmlns:core="sap.ui.core"
                                                                                    id="mivop"
                                                                                    key="nameProyect"
                                                                                    value="{nameProyect}"
                                                                                />

                                                                                <core:CustomData
                                                                                    xmlns:core="sap.ui.core"
                                                                                    key="projectId"
                                                                                    value="{ID}"
                                                                                />
                                                                            </customData>
                                                                        </Button>
                                                                    </items>
                                                                </HBox>
                                                            </cells>
                                                        </ColumnListItem>
                                                    </items>
                                                </Table>
                                            </IconTabFilter>
                                            <IconTabFilter
                                                id="ma55"
                                                key="notes"
                                                icon="sap-icon://notes"
                                                tooltip="Solicitudes Aprobadas"
                                                count=""
