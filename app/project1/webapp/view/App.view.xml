                                                                <Text
                                                                    id="2333d"
                                                                    text="{codigoProyect}"
                                                                />
                                                                <Text
                                                                    id="00"
                                                                    text="{nameProyect}"
                                                                />
                                                                <Text
                                                                    id="0"
                                                                    text=""
                                                                />
                                                                <Text
                                                                    id="20"
                                                                    text="{Area/NombreArea}"
                                                                />
                                                                <Text
                                                                    id="210"
                                                                    text="{jefeProyectID/name}"
                                                                />

                                                                <ObjectStatus
                                                                    id="status02"
                                                                    text="{Estado}"
