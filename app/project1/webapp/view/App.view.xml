                                                <Table
                                                    id="idProductsTable"
                                                    sticky="HeaderToolbar,ColumnHeaders"
                                                    inset="false"
                                                    items="{/DatosProyect}"
                                                    class="sapFDynamicPageAlignContent aprobado"
                                                    width="auto"
                                                >
                                                    <headerToolbar>
                                                        <Toolbar id="d">
                                                            <Title
                                                                id="d2"
                                                                text="Lista De Solicitudes"
                                                                level="H2"
                                                            />
                                                            <SearchField
                                                                id="searchField2"
                                                                liveChange="onSearchApro"
