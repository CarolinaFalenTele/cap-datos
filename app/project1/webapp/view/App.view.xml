                                                                />
                                                            </cells>
                                                        </ColumnListItem>
                                                    </items>
                                                </Table>
                                            </IconTabFilter>
                                            <IconTabFilter
                                                id="ma77"
                                                key="people"
                                                tooltip="Seguimiento de Proceso"
                                                icon="sap-icon://process"
                                            >
                                                <Panel
                                                    xmlns="sap.m"
                                                    width="auto"
                                                    expandable="true"
                                                    expanded="true"
                                                    id="panel5887540"
                                                >
                                                    <headerToolbar>
                                                        <Toolbar
                                                            id="toolbar1_1728554177158"
                                                        >
                                                            <Title
                                                                id="idTitleProceso"
                                                                text="Proceso"
                                                            />

                                                            <ToolbarSpacer
                                                                id="jdfkjsdfs"
                                                            />

                                                            <Button
                                                                id="kfsldkfjwo"
                                                                icon="sap-icon://settings"
                                                            />
                                                        </Toolbar>
                                                    </headerToolbar>

                                                    <content>
                                                        <Text
                                                            text="Proceso de Solictud"
                                                            id="text1881"
                                                        />

                                                        <suite:ProcessFlow
                                                            id="processflow1"
                                                            nodePress="onNodePress"
                                                        >
                                                            <suite:nodes>
                                                                <!-- Nodo principal -->
                                                                <suite:ProcessFlowNode
                                                                    id="mainNode"
                                                                    nodeId="0"
                                                                    laneId="0"
                                                                    title="{nameProyect}"
                                                                    titleAbbreviation="SO 1"
                                                                    isTitleClickable="false"
                                                                    state="Positive"
                                                                    stateText="En Progreso"
                                                                    texts=""
                                                                    children="1"
                                                                />

                                                                <!-- Nodo secundario: "Pendiente de Aprobación" -->
                                                                <suite:ProcessFlowNode
                                                                    id="approvalNode"
                                                                    nodeId="1"
                                                                    laneId="1"
                                                                    title="Pendiente de Aprobación"
                                                                    titleAbbreviation="PA"
                                                                    isTitleClickable="false"
                                                                    state="Neutral"
                                                                    stateText="Esperando Aprobación"
                                                                    texts=""
                                                                    children="2"
                                                                />

                                                                <!-- Nodo secundario: "Revisión de Basis" -->
                                                                <suite:ProcessFlowNode
                                                                    id="basisNode"
                                                                    nodeId="2"
                                                                    laneId="2"
                                                                    title="Nota de Basis"
                                                                    titleAbbreviation="NB"
                                                                    isTitleClickable="false"
                                                                    state="Negative"
                                                                    stateText="Revisión Necesaria"
                                                                    texts="Nota adicional sobre Basis"
                                                                    children="3"
                                                                />

                                                                <!-- Nodo final: "Aprobado" -->
                                                                <suite:ProcessFlowNode
                                                                    id="approvedNode"
                                                                    nodeId="3"
                                                                    laneId="8"
                                                                    title="Aprobado"
                                                                    titleAbbreviation="APR"
                                                                    isTitleClickable="false"
                                                                    state="Positive"
                                                                    stateText="Aprobación Completa"
                                                                    texts=""
                                                                />
                                                            </suite:nodes>

                                                            <!-- Definir las lanes -->
                                                            <suite:lanes>
                                                                <suite:ProcessFlowLaneHeader
                                                                    id="lane0"
                                                                    laneId="0"
                                                                    iconSrc="sap-icon://crm-service-manager"
                                                                    text="Solicitud CDO"
                                                                    position="0"
                                                                />
                                                                <suite:ProcessFlowLaneHeader
                                                                    id="lane1"
                                                                    laneId="1"
                                                                    iconSrc="sap-icon://approvals"
                                                                    text="Pendiente de Aprobación"
                                                                    position="1"
                                                                />
                                                                <suite:ProcessFlowLaneHeader
                                                                    id="lane2"
                                                                    laneId="2"
                                                                    iconSrc="sap-icon://notes"
                                                                    text="Requiere Basis/ TQ/ Factoria"
                                                                    position="2"
                                                                />
                                                                <suite:ProcessFlowLaneHeader
                                                                    id="lane3"
                                                                    laneId="3"
                                                                    iconSrc="sap-icon://testing"
                                                                    text="Revision PMO"
                                                                    position="3"
                                                                />
                                                                <suite:ProcessFlowLaneHeader
                                                                    id="lane4"
                                                                    laneId="4"
                                                                    iconSrc="sap-icon://factory"
                                                                    text="Aprobado PMO"
                                                                    position="4"
                                                                />
                                                                <suite:ProcessFlowLaneHeader
                                                                    id="lane5"
                                                                    laneId="5"
                                                                    iconSrc="sap-icon://calendar"
                                                                    text="Revision Control de Gestion"
                                                                    position="5"
                                                                />
                                                                <suite:ProcessFlowLaneHeader
                                                                    id="lane6"
                                                                    laneId="6"
                                                                    iconSrc="sap-icon://accept"
                                                                    text="Aprobado de Gesion"
                                                                    position="6"
                                                                />
                                                                <suite:ProcessFlowLaneHeader
                                                                    id="lane8"
                                                                    laneId="8"
                                                                    iconSrc="sap-icon://message-error"
                                                                    text="Revision Direccion"
                                                                    position="7"
                                                                />
                                                                <suite:ProcessFlowLaneHeader
                                                                    id="lane9"
                                                                    laneId="9"
                                                                    iconSrc="sap-icon://payment-approval"
                                                                    text="Aprobación Dirección"
                                                                    position="8"
                                                                />
                                                            </suite:lanes>
                                                        </suite:ProcessFlow>
                                                    </content>
                                                </Panel>
                                            </IconTabFilter>
                                        </items>

                                        <content />
                                    </IconTabBar>
                                </headerContainer>
                            </ObjectHeader>
                        </items>
                    </VBox>
                </content>

                <footer>
                    <OverflowToolbar id="toolbar1">
                        <ToolbarSpacer id="22" />

                        <Button
                            id="33"
                            type="Accept"
                            text="Nueva Solicitud"
