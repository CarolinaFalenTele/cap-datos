                                                                             <core:CustomData
                                                                                    xmlns:core="sap.ui.core"
                                                                                    id="mivrf5op"
                                                                                    key="nameProyect"
                                                                                    value="{nameProyect}"
                                                                                />
                                                                            <core:CustomData
                                                                                xmlns:core="sap.ui.core"
                                                                                key="projectId"
                                                                                value="{ID}"
                                                                            />
                                                                        </customData>
                                                                    </Button>

                                                                    <Button
                                                                        id="ww"
                                                                        icon="sap-icon://delete"
                                                                        press="onDeletePress"
                                                                    >
                                                                        <customData
                                                                        >
                                                                            <core:CustomData
                                                                                xmlns:core="sap.ui.core"
                                                                                key="projectId"
                                                                                value="{ID}"
                                                                            />
                                                                        </customData>
                                                                    </Button>

                                                                    <items>
                                                                        <Button
                                                                            xmlns="sap.m"
                                                                            icon="sap-icon://activity-2"
                                                                            text=""
