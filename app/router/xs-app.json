{
    "authenticationMethod": "route",
    "logout": {
        "logoutEndpoint": "/app-logout",
        "logoutPage": "/"
    },
    "routes": [
        {
            "source": "^/keyuser/(.*)$",
	"target": "$1",
	"service": "com.sap.ui.flex.cf.keyuser",
	"endpoint": "keyuser"
        },
        {

            
            "source": "^/app/(.*)$",
            "target": "$1",
            "localDir": ".",
            "cacheControl": "no-cache, no-store, must-revalidate",
            "authenticationType": "xsuaa"
        },
        {
            "source": "^/appconfig/",
            "localDir": ".",
            "cacheControl": "no-cache, no-store, must-revalidate"
        },
        {
            "source": "^/user-api(.*)",
            "target": "$1",
            "service": "sap-approuter-userapi"
        },
        {
            "source": "^/(.*)$",
            "target": "$1",
            "destination": "srv-api",
            "csrfProtection": true,
            "authenticationType": "xsuaa"
        }
    ]
    }
    