{
  "name": "datosCdo",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/hana": "^1",
    "@sap-cloud-sdk/connectivity": "^3.24.0",
    "@sap/approuter": "^20.3.1",
    "@sap/cds": "^8.7.0",
    "@sap/cds-dk": "^8.7.0",
    "@sap/cds-hana": "^2.0.0",
    "@sap/hana-client": "^2",
    "@sap/xsenv": "^5.5.0",
    "@sap/xssec": "^4.4.0",
    "@ui5/logger": "^4.0.1",
    "dotenv": "^16.4.5",
    "express": "^4",
    "glob": "^11.0.1",
    "hdb": "^0.19.9"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^1",
    "@sap/eslint-plugin-cds": "^3.0.5",
    "@sap/hdi-deploy": "^5.3.2",
    "@sap/html5-repo-mock": "^2.1.10",
    "@sap/ux-specification": "^1.120.15",
    "eslint": "^9.11.1",
    "rimraf": "^6.0.1"
  },
  "scripts": {
    "start": "cds-serve",
    "deploy": "cf deploy mta_archives/datosCdo_1.0.0.mtar -f --delete-services || exit 1",
    "dev": "cd app/router && node node_modules/@sap/approuter/approuter.js",
    "watch-project1": "cds watch --open project1/webapp/index.html?sap-ui-xx-viewCache=false",
    "undeploy": "cf undeploy datosCdo --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive"
  },
  "cds": {
    "features": {
      "fiori_preview": true
    },
    "server": {
      "index": true
    },
    "requires": {
      "db": "hana",
      "auth": "xsuaa",
      "html5-repo": true
    },
    "hana": {
      "deploy-format": "hdbtable"
    },
    "sql": {
      "native_hana_associations": false
    }
  }
}
